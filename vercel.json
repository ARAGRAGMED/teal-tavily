{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "api/search/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "api/search/index.py"
    }
  ]
}